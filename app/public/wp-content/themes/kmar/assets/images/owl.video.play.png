        var dotsLength = dots.length;
        for (var i = 0; i < dotsLength; i++) {
            var dot = dots.eq(i);
            dot.addClass('dotnumber' + dotcount);
            dot.attr('data-info', dotcount);
            dotcount++;
        }

        var slidecount = 1;
        var slides = $('.owl-item').not('.cloned');
        var slidesLength = slides.length;
        for (var j = 0; j < slidesLength; j++) {
            var slide = slides.eq(j);
            slide.addClass('slidenumber' + slidecount);
            slidecount++;
        }
    });
</script>

<script type="text/javascript">
    function countUp(target, start, end, duration) {
        let current = start;
        const range = end - start;
        const increment = end > start ? 1 : -1;
        const stepTime = Math.abs(Math.floor(duration / range));
        const element = typeof target === 'string' ? document.querySelector(target) : target; // Check if target is a string or DOM element
        const timer = setInterval(function() {
            current += increment;
            element.textContent = current + '+';
            if (current === end) {
                clearInterval(timer);
            }
        }, stepTime);
    }


    function isElementInViewport(el) {
        const rect = el.getBoundingClientRect();
        return (
            rect.top >= 0 &&
            rect.left >= 0 &&
            rect.bottom <= (window.innerHeight || document.documentElement.clientHeight) &&
            rect.right <= (window.innerWidth || document.documentElement.clientWidth)
        );
    }

    function countUpWhenVisible() {
        const elements = document.querySelectorAll('.count-up-trigger');
        elements.forEach(function(element, index) {
            if (isElementInViewport(element)) {
                const countElement = element.querySelector('.v--number--hot');
                const endValue = parseInt(countElement.textContent);
                const startValue = index === 2 ? endValue / 2 : 0;

                const duration = index === 2 ? 3 : 1000; // Thời gian đếm là 2000 ms cho phần tử thứ 2, 1000 ms cho các phần tử khác
                countUp(countElement, startValue, endValue, duration);
                if (index == 2) {
                    // console.log(countElement);
                }
                element.classList.remove('count-up-trigger'); // Prevent counting up again for the same element
            }
        });
    }
    var temp = $('.v--number--hot');
    if (temp.length > 0) {
        // console.log('Temp exists: ' + temp);
        window.onload = function() {
            countUp('.v--number--hot', 0, parseInt(document.querySelector('.v--number--hot').textContent), 1000);
            countUpWhenVisible();
        };
    }

    window.addEventListener('scroll', countUpWhenVisible);
    window.addEventListener('resize', countUpWhenVisible);
    // var temp2 = $('.swiper-slide-active .v--number--hot')
    // if (temp2.length > 0) {
    //     countUp('.v--number--hot', 0, parseInt(document.querySelector('.v--number--hot').textContent), 1000);
    //     countUpWhenVisible();
    // }

    // console.log('TEMP @ :'+temp2);
</script>


<script type="text/javascript">
    function checkScreenSize() {
        if (window.innerHeight >= 680 && window.innerWidth >= 1400 && window.innerWidth <= 1930)   {
            var swiper = new Swiper('.my-swiper', {
                direction: 'vertical',
                slidesPerView: 1,
                spaceBetween: 60,
                mousewheel: true,
                grabCursor: true,
                loop: false,
                speed: 1000,
            });
        } else {
            var my_swiper = document.querySelector('.my-swiper');
            var swiper_wrapper = document.querySelector('.my-swiper .swiper-wrapper');
            var swiper_slides = document.querySelectorAll('.my-swiper .swiper-slide');
            // my_swiper.classList.remove('my-swiper');
            swiper_wrapper.classList.remove('swiper-wrapper');
            swiper_slides.forEach(function(slide) {
                slide.classList.remove('swiper-slide');
            });
        }
        // console.log('Width : ' + window.innerWidth);
        // console.log('Height : ' + window.innerHeight);

        // 
    }

    function reloadOnResize() {
        location.reload();
    }
  if (window.innerWidth >= 1399) {
        window.addEventListener('resize', reloadOnResize);
    }
    
    window.addEventListener('load', checkScreenSize);
    window.addEventListener('resize', checkScreenSize);
</script>


<script type="text/javascript">
    window.addEventListener('scroll', function() {
        var elements = document.querySelectorAll('.cus--roadmap--curriculum');
        elements.forEach(function(element) {
            var position = element.getBoundingClientRect();
            if (position.top < window.innerHeight) {
                $('.cus--roadmap').addClass('show');
                $('.cus--curriculum').addClass('show');
            } else {
                $('.cus--roadmap').removeClass('show');
                $('.cus--curriculum').removeClass('show');
            }
        });
    });
</script>
